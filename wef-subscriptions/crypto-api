<Subscription xmlns="http://schemas.microsoft.com/2006/03/windows/events/subscription">
  <SubscriptionId>Crypto-API</SubscriptionId>
  <SubscriptionType>SourceInitiated</SubscriptionType>
  <Description>Crypto CAPI</Description>
  <Enabled>true</Enabled>
  <Uri>http://schemas.microsoft.com/wbem/wsman/1/windows/EventLog</Uri>
  <ConfigurationMode>Custom</ConfigurationMode>
  <Delivery Mode="Push">
    <Batching>
      <MaxItems>5</MaxItems>
      <MaxLatencyTime>300000</MaxLatencyTime>
    </Batching>
    <PushSettings>
      <Heartbeat Interval="1800000"/>
    </PushSettings>
  </Delivery>
  <Query><![CDATA[
    <QueryList>
      <!-- Inspired by Microsoft Documentation -->
      <!-- Event folder disabled per default -->
      <Query Id="0" Path="Microsoft-Windows-CAPI2/Operational">
        <!-- 11: Cert Trust Chain Build Failed -->
        <Select Path="Microsoft-Windows-CAPI2/Operational">*[System[(EventID=11)]]</Select>
        <!-- 70: Private Key Accessed -->
        <Select Path="Microsoft-Windows-CAPI2/Operational">*[System[(EventID=70)]]</Select>
        </Query>
    </QueryList>]]></Query>
  <ReadExistingEvents>true</ReadExistingEvents>
  <TransportName>http</TransportName>
  <ContentFormat>Events</ContentFormat>
  <Locale Language="en-US"/>
  <LogFile>WEC5-Crypto-API</LogFile>
  <AllowedSourceNonDomainComputers/>
  <AllowedSourceDomainComputers>O:NSG:NSD:(A;;GA;;;DC)(A;;GA;;;NS)(A;;GA;;;DD)</AllowedSourceDomainComputers>
</Subscription>
